{
  "name": "sick-fits-backend",
  "version": "1.0.0",
  "description": "the backend of sick fits",
  "scripts": {
    "start": "NODE_PATH=./dist NODE_ENV=production node ./dist/src/index.js",
    "dev": "ts-node-dev --trace-warnings --respawn --no-notify -r tsconfig-paths/register src/index.ts",
    "type": "tsc --watch",
    "lint": "esw -w --ext .ts,.tsx . --fix",
    "test": "jest",
    "style": "npm-run-all --parallel type lint"
  },
  "dependencies": {
    "apollo-server-express": "^2.12.0",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.1.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.4.6",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "pg": "^8.0.3",
    "reflect-metadata": "^0.1.13",
    "stripe": "^8.55.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24"
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "env"
    ]
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
